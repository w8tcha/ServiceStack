{
  "name": "my-app",
  "version": "1.0.0",
  "scripts": {
    "dev": "dotnet watch",
    "path": "which tailwindcss",
    "ui:dev": "tailwindcss -i index.css -o wwwroot/css/ui.css --watch",
    "ui:build": "tailwindcss -i index.css -o wwwroot/css/ui.css --minify",
    "migrate": "dotnet run --AppTasks=migrate",
    "revert:last": "dotnet run --AppTasks=migrate.revert:last",
    "revert:all": "dotnet run --AppTasks=migrate.revert:all"
  }
}
